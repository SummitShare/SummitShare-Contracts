{
  "name": "@rev-share/contracts",
  "version": "0.0.1",
  "description": "SummitShare Platfrom Smart Contracts(RVS-m)",
  "scripts": {
    "i": "yarn install",
    "test": "REPORT_GAS=true hardhat test",
    "test:coverage": "hardhat coverage --solcoverjs ./.solcover.js --temp build/contracts --testfiles \"./test/*.ts\"",
    "build": "hardhat compile",
    "deploy:dev": "hardhat run --network sepolia scripts/deploy.ts",
    "configure:dev": "hardhat run --network sepolia scripts/configure.ts",
    "mock:dev": "hardhat run --network sepolia scripts/mock.ts"
  },
  "keywords": ["solidity", "SummitShare", "Ethereum", "erc721a"],
  "author": "https://github.com/SummitShare",
  "license": "MIT",
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.2",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.11",
    "@nomicfoundation/hardhat-toolbox": "^3.0.0",
    "@nomiclabs/hardhat-ethers": "^2.2.3",
    "@openzeppelin/contracts-upgradeable": "^5.0.1",
    "@openzeppelin/hardhat-upgrades": "^3.0.2",
    "@types/chai": "^4.3.9",
    "@types/mocha": "^10.0.3",
    "@types/node": "^20.8.7",
    "chai": "^4.3.10",
    "dotenv": "^16.3.1",
    "erc721a": "^4.2.3",
    "ethers": "^6.13.1",
    "hardhat": "^2.22.6",
    "hardhat-contract-sizer": "^2.10.0",
    "solidity-coverage": "^0.8.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@nomicfoundation/hardhat-verify": "^2.0.8",
    "@openzeppelin/contracts": "^5.0.0"
  },
  "repository": "https://github.com/SummitShare/SummitShare-Contracts",
  "private": false
}
